{
  "name": "cetus-optimizer",
  "version": "1.0.0",
  "description": "Automated liquidity management system for Cetus DEX on Sui blockchain",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "db:init": "ts-node scripts/init-db.ts",
    "verify": "ts-node scripts/verify-deployment.ts",
    "healthcheck": "node -e \"require('fs').existsSync('data/optimizer.db') && process.exit(0) || process.exit(1)\""
  },
  "keywords": [
    "sui",
    "cetus",
    "defi",
    "liquidity",
    "automation"
  ],
  "author": "Ben Putley",
  "license": "MIT",
  "dependencies": {
    "@cetusprotocol/cetus-sui-clmm-sdk": "^5.0.0",
    "@mysten/sui": "^1.0.0",
    "better-sqlite3": "^12.5.0",
    "decimal.js": "^10.4.0",
    "dotenv": "^16.0.0",
    "node-telegram-bot-api": "^0.64.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.0.0",
    "@types/node": "^20.0.0",
    "@types/node-telegram-bot-api": "^0.64.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
